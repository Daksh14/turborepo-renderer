{
  "name": "cms-render-template",
  "private": true,
  "scripts": {
    "build": "bun run turbo run build",
    "dev": "bun run turbo run dev",
    "lint": "bun run turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "bun run turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.1",
    "typescript": "5.9.2",
    "web": "workspace:*"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.6",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
